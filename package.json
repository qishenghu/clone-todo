{
    "name": "zenstack-todo-sample-nextjs",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev -p 3008",
        "build": "npm run generate && next build",
        "start": "next start",
        "lint": "next lint",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:deploy": "prisma migrate resolve --applied 0_init",
        "db:reset": "prisma migrate reset",
        "db:browse": "prisma studio",
        "generate": "zenstack generate",
        "vercel-build": "npm run build && npm run db:deploy",
        "package-clean": "npm rm zenstack @zenstackhq/runtime @zenstackhq/server @zenstackhq/swr",
        "up": "npm run package-clean && npm install -D --save-exact zenstack@latest @zenstackhq/swr@latest && npm install --save-exact @zenstackhq/runtime@latest @zenstackhq/server@latest",
        "up-preview": "npm run package-clean && npm install --registry https://dolphin-app-nyvr5.ondigitalocean.app -D zenstack@latest @zenstackhq/swr@latest && npm install --registry https://dolphin-app-nyvr5.ondigitalocean.app @zenstackhq/runtime@latest @zenstackhq/server@latest"
    },
    "dependencies": {
        "@heroicons/react": "^2.0.12",
        "@next-auth/prisma-adapter": "^1.0.6",
        "@prisma/client": "^5.0.0",
        "@tanstack/react-query": "^4.20.2",
        "@vercel/analytics": "^1.0.1",
        "@zenstackhq/runtime": "1.0.0-beta.21",
        "@zenstackhq/server": "1.0.0-beta.21",
        "babel-plugin-superjson-next": "^0.4.5",
        "bcryptjs": "^2.4.3",
        "daisyui": "^2.31.0",
        "moment": "^2.29.4",
        "nanoid": "^4.0.0",
        "next": "^13.4.4",
        "next-auth": "^4.22.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-toastify": "^9.0.8",
        "superjson": "^1.12.0",
        "swr": "^2.1.3"
    },
    "devDependencies": {
        "@tailwindcss/line-clamp": "^0.4.2",
        "@types/bcryptjs": "^2.4.2",
        "@types/node": "^14.17.3",
        "@types/react": "18.0.21",
        "@types/react-dom": "18.0.6",
        "@zenstackhq/swr": "1.0.0-beta.21",
        "autoprefixer": "^10.4.12",
        "eslint": "^7.19.0",
        "eslint-config-next": "12.3.1",
        "postcss": "^8.4.16",
        "prisma": "^5.0.0",
        "tailwindcss": "^3.1.8",
        "typescript": "^5.1.6",
        "zenstack": "1.0.0-beta.21"
    }
}
